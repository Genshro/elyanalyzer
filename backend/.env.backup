# ElyAnalyzer Backend Environment Variables

# Supabase Configuration (Required)
SUPABASE_URL=https:your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key

# Server Configuration
PORT=8080
ENVIRONMENT=production

# Security Settings
JWT_SECRET=your-super-secret-jwt-key-here-minimum-32-chars
ALLOWED_ORIGINS=https://www.elyanalyzer.com,http://www.elyanalyzer.com,https://elyanalyzer.com,http://elyanalyzer.com

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_BURST=100

# File Upload Settings
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=.js,.ts,.go,.py,.java,.cpp,.c,.rs,.php,.rb,.cs,.swift,.kt,.dart

# Database Settings
DB_POOL_SIZE=10
DB_MAX_IDLE_CONNECTIONS=5
DB_CONNECTION_TIMEOUT=30

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/elyanalyzer.log
